{
  "expo": {
    "name": "react-native__todo-target-dream",
    "slug": "todo-target-dream",
    "owner": "hyeonwooga",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
